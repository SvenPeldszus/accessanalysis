<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.views">
      <view
            category="de.uni_hamburg.informatik.swt.accessanalysis"
            class="de.uni_hamburg.informatik.swt.accessanalysis.resultsview.AccessAnalysisView"
            icon="icons/access.png"
            id="de.uni_hamburg.informatik.swt.accessanalysis.resultsview"
            name="AccessAnalysis"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="org.eclipse.jdt.ui.JavaPerspective">
         <view
               id="de.uni_hamburg.informatik.swt.accessanalysis.resultsview"
               minimized="false"
               relationship="stack"
               relative="org.eclipse.ui.views.ProblemView"
               visible="false">
         </view>
      </perspectiveExtension>
   </extension>
   <extension
         point="de.uni_hamburg.informatik.swt.accessanalysis.results.consumer">
      <consumer
            class="de.uni_hamburg.informatik.swt.accessanalysis.resultsview.AccessAnalysisViewFiller">
      </consumer>
   </extension>
	<extension
         point="org.eclipse.ui.handlers">
      <handler
            class="de.uni_hamburg.informatik.swt.accessanalysis.clipboard.ClipboardHandler"
            commandId="org.eclipse.ui.edit.copy">
                     <activeWhen>
	            <with
	                  variable="activePartId">
	               <equals
	                     value="de.uni_hamburg.informatik.swt.accessanalysis.resultsview">
	               </equals>
	            </with>
	        </activeWhen>
	        <enabledWhen>
	            <with
	                  variable="selection">
	               <count
	                     value="+">
	               </count>
	            </with>
	        </enabledWhen>
      </handler>
   </extension>
</plugin>
